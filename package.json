{
  "name": "@sultanyaron/component-generator",
  "version": "0.5.0",
  "private": false,
  "description": "A component generator CLI for redis FE developers",
  "main": "dist/cli.js",
  "type": "module",
  "bin": {
    "component-generator": "bin/component-generator.js"
  },
  "lint-staged": {
    "src/**/*": [
      "npm run eslint",
      "npm run prettier:write"
    ]
  },
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "dev": "rollup -c rollup.config.js --watch",
    "test": "jest --watch",
    "test:ci": "jest",
    "prettier:write": "prettier --write ./src",
    "prepare": "husky install",
    "eslint": "eslint src --color",
    "types-check": "tsc"
  },
  "keywords": [],
  "author": "sultanyaron@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-typescript": "^9.0.1",
    "@types/inquirer": "^9.0.2",
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.2",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "eslint": "^8.26.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.1",
    "jest": "^29.2.1",
    "lint-staged": "^13.0.3",
    "prettier": "2.7.1",
    "rollup": "^3.2.3",
    "ts-jest": "^29.0.3",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "arg": "^5.0.2",
    "inquirer": "^9.1.4",
    "tree-node-cli": "^1.5.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sultanyaron/component-generator.git"
  },
  "bugs": {
    "url": "https://github.com/Sultanyaron/component-generator/issues"
  },
  "homepage": "https://github.com/Sultanyaron/component-generator#readme"
}
