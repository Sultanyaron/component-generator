{
  "env": {
    "browser": true,
    "es2021": true
  },
  "overrides": [],
  "extends": ["standard-with-typescript", "prettier", "eslint-config-prettier"],
  "plugins": ["prettier", "@typescript-eslint"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": ["./tsconfig.json"]
  },
  "rules": {
    "@typescript-eslint/explicit-function-return-type": "off"
  }
}
